{
  "version": 2,
  "builds": [
    {
      "src": "Client/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "Backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Client/dist/$1"
    }
  ]
}
